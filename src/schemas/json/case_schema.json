{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "A product from Acme's catalog",
  "definitions": {
    "api": {
      "title": "api url info",
      "type": "object",
      "properties": {
        "host": {
          "type": "string",
        },
        "uri": {
          "type": "string"
        },
        "method": {
          "type": "string"
        },
        "header": {
          "type": "string",
          "default": "https://dev-api.qiandao.cn"
        },
      }
    },
    "template":{
      "title": "template for all request",
      "type": "object",
      "properties": {
        "header": {
          "type": "object",
          "properties":{
            "authorization": {
              "type": "object",
              "default": "${{get_user_token(18669420998)}}"
            }
          }
        },
      }
    }
  },
  "title": "case",
  "type": "object"
}
